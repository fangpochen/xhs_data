# 小红书Cookie配置（可选）
XHS_COOKIE=your_cookie_here

# 代理配置（可选）
HTTP_PROXY=http://your_proxy_here
HTTPS_PROXY=http://your_proxy_here

# 采集配置
MAX_RETRY=5
TIMEOUT=10
CHUNK_SIZE=1024*1024

# 存储配置
DATA_PATH=./datas
EXCEL_PATH=./datas/excel
MEDIA_PATH=./datas/media

# 日志配置
LOG_LEVEL=INFO
LOG_PATH=./datas/logs

# 采集频率控制
REQUEST_DELAY=1
MAX_REQUESTS_PER_MINUTE=60

# 其他配置
USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/122.0.0.0 Safari/537.36 